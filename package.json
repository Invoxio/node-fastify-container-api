{
  "name": "fastify-container-api-framework",
  "version": "0.0.1",
  "description": "Node based fastify framework for API development in a container",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "fastify start -l info app.js",
    "test": "jest --coverage --runInBand ",
    "test:watch": "jest --watch --runInBand",
    "debug": "fastify start -d -I 9229 -l info app.js",
    "dev": "fastify start -w --ignore-watch='data' -l info -P app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Invoxio/node-fastify-container-api-framework.git"
  },
  "contributors": [
    "Timothy Meade <tim@invoxio.com>"
  ],
  "bugs:": "https://github.com/Invoxio/node-fastify-container-api-framework/issues",
  "engines": {
    "node": "^14.0 || >=10.*"
  },
  "keywords": [
    "node",
    "fastify",
    "container",
    "api",
    "vscode"
  ],
  "author": "Timothy Meade <tim@invoxio.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Invoxio/node-fastify-container-api-framework/issues"
  },
  "homepage": "https://github.com/Invoxio/node-fastify-container-api-framework#readme",
  "dependencies": {
    "@hapi/boom": "^9.1.3",
    "dotenv": "^10.0.0",
    "fastify": "^3.19.2",
    "fastify-autoload": "^3.8.0",
    "fastify-cli": "^2.13.0",
    "fastify-cookie": "^5.3.1",
    "fastify-plugin": "^3.0.0",
    "fastify-session": "^5.2.1",
    "prettier": "^2.3.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "pino-pretty": "^5.1.2"
  }
}
